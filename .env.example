# Inworld AI API Key
# Get this from your Inworld workspace
INWORLD_API_KEY=your_api_key_here

# Zoom RTMS Credentials
# Get these from your Zoom App Marketplace page
ZM_RTMS_CLIENT=your_zoom_client_id_here
ZM_RTMS_SECRET=your_zoom_client_secret_here

# Logging Configuration (optional)
# App logging: ERROR or DEBUG
LOG_LEVEL=ERROR

# RTMS SDK logging: disabled, error, warn, info, or debug
RTMS_LOG_LEVEL=disabled

# Server Port (optional)
PORT=3000
